{
  "name": "nightcall-hackathon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:analyser": "cross-env NODE_ENV=production BUNDLE_ANALYSER=true webpack --config config/webpack.prod.js",
    "test": "react-scripts test --passWithNoTests",
    "test:code": "react-scripts test --watchAll=false --passWithNoTests",
    "test:upd:snap": "react-scripts test --watchAll=false -u --passWithNoTests",
    "serve": "serve build -s -p 5000",
    "lint": "eslint src",
    "prepare": "husky install",
    "type:coverage": "type-coverage --detail"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@hookform/resolvers": "^2.9.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@mui/styles": "^5.9.3",
    "bulma": "^0.9.4",
    "date-fns": "2.28.0",
    "firebase": "^9.9.4",
    "ga-4-react": "^0.1.281",
    "path-to-regexp": "6.2.1",
    "qs": "6.10.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "3.1.4",
    "react-firebase-hooks": "^5.0.3",
    "react-hook-form": "7.32.2",
    "react-router-dom": "6.3.0",
    "react-use": "17.4.0",
    "recoil": "^0.7.5",
    "sanitize.css": "13.0.0",
    "sass": "^1.54.4",
    "type-fest": "2.13.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/plugin-transform-typescript": "^7.18.4",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "^7.17.12",
    "@svgr/webpack": "6.1.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.1",
    "react-scripts": "^5.0.1",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.6.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "git-branch-is": "^4.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "mini-css-extract-plugin": "2.5.3",
    "prettier": "^2.7.1",
    "sass-loader": "^13.0.2",
    "terser-webpack-plugin": "5.3.1",
    "ts-loader": "9.3.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "type-coverage": "^2.22.0",
    "typescript": "^4.7.4",
    "url-loader": "4.1.1",
    "webpack": "5.69.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.7.4",
    "webpack-merge": "5.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
