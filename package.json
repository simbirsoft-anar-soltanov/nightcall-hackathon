{
  "name": "nightcall-hackathon",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:analyser": "cross-env NODE_ENV=production BUNDLE_ANALYSER=true webpack --config config/webpack.prod.js",
    "test": "react-scripts test --passWithNoTests",
    "test:code": "react-scripts test --watchAll=false --passWithNoTests",
    "test:upd:snap": "react-scripts test --watchAll=false -u --passWithNoTests",
    "serve": "serve build -s -p 5000",
    "lint": "eslint src",
    "prepare": "husky install",
    "type:coverage": "type-coverage --detail"
  },
  "dependencies": {
    "@emailjs/browser": "^3.10.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.10",
    "@mui/styles": "^5.9.3",
    "bulma": "^0.9.4",
    "date-fns": "2.29.3",
    "firebase": "^9.12.1",
    "ga-4-react": "^0.1.281",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "3.1.4",
    "react-firebase-hooks": "^5.0.3",
    "react-hook-form": "7.38.0",
    "react-router-dom": "6.3.0",
    "react-scroll": "^1.8.8",
    "react-use": "17.4.0",
    "recoil": "^0.7.6",
    "sanitize.css": "13.0.0",
    "sass": "^1.55.0",
    "type-fest": "3.1.0",
    "use-pagination-firestore": "^0.6.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/plugin-transform-typescript": "^7.19.3",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@svgr/webpack": "6.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.1",
    "@types/react-scroll": "^1.8.4",
    "@types/uuid": "^8.3.4",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.2.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "git-branch-is": "^4.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "2.6.1",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "sass-loader": "^13.1.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.4.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "type-coverage": "^2.22.0",
    "typescript": "^4.8.4",
    "url-loader": "4.1.1",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.6.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
